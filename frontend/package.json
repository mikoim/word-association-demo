    "vue-router": "^3.5.3",
    "vuetify": "^2.6.10",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.25",
    "@types/set-interval-async": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
